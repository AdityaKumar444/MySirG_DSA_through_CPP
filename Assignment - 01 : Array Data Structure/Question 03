#include<bits/stdc++.h>
using namespace std ;
class Array
{
    private:
        int capacity , last_index=-1 , *ptr ;
    public:
        bool emptyOrNot();
        Array(){}
        Array(int n)
        {
            capacity = n ;
            ptr = new int[capacity];
        }
        ~Array()
        {
            delete[]ptr;
        }
};
bool Array::emptyOrNot()
{
    if(last_index==-1)
    return true ;
    else
    return false ;
}
